{
  "auth": {
    "rolesSource": "/api/auth/me",
    "identityProviders": {
      "customOpenIdConnectProviders": {}
    }
  },
  "routes": [
    {
      "route": "/api/auth/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/videos/shared/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/auth/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/auth/signin"
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.{css,scss,sass,js,json,ico,png,jpg,jpeg,gif,svg,woff,woff2}"]
  },
  "platform": {
    "apiRuntime": "node:18"
  }
} 